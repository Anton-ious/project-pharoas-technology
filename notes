<!--Animation CSS CODE-->   

.animated {
    position: relative;
    top: 0px;
    animation: animated1 4s linear infinite;

}

.animte {
    width: 50px;
    z-index: 2;
    opacity: 0.4;
}

.animte:hover {
    border-radius: 10px;
    width: 60px;
    filter: drop-shadow(5px 1px 9px rgb(0, 0, 0));
    opacity: 1;

}

.animte1 {
    position: absolute;
    left: 20%;
    top: 20px;

}

@keyframes animated1 {
    0% {
        position: absolute;
        top: 0%;
        left: 30%;
        transform: rotatez(10deg);
        display: none;
        z-index: 1;

    }

    50% {
        position: absolute;
        top: 30%;
        left: 30%;
        transform: rotatez(20deg);
        z-index: -1;
        display: block;
    }

    70% {
        position: absolute;
        top: 50%;
        left: 30%;
        transform: rotatez(30deg);
        z-index: -1;
    }

    100% {
        position: absolute;
        top: 80%;
        left: 30%;
        transform: rotatez(10deg);
        display: none;
        z-index: 1;
    }
}

